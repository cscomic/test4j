<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
	<modelVersion>4.0.0</modelVersion>
	<groupId>org.test4j</groupId>
	<artifactId>test4j.tutorial</artifactId>
	<packaging>jar</packaging>
	<version>2.0.0</version>
	<name>test4j tutorial</name>

	<properties>
		<file_encoding>utf-8</file_encoding>
		<test4j.version>2.0.0</test4j.version>
	</properties>

	<dependencies>
		<dependency>
			<groupId>org.test4j</groupId>
			<artifactId>test4j.testng</artifactId>
			<version>${test4j.version}</version>
		</dependency>
		<dependency>
			<groupId>org.test4j</groupId>
			<artifactId>test4j.junit</artifactId>
			<version>${test4j.version}</version>
		</dependency>
		<dependency>
			<groupId>org.test4j</groupId>
			<artifactId>test4j.integrated</artifactId>
			<version>${test4j.version}</version>
		</dependency>
		<dependency>
			<groupId>org.test4j</groupId>
			<artifactId>test4j.integrated</artifactId>
			<type>test-jar</type>
			<version>${test4j.version}</version>
			<scope>test</scope>
		</dependency>
		<dependency>
			<groupId>cp-suite</groupId>
			<artifactId>cpsuite.file</artifactId>
			<version>UNKOWN</version>
			<scope>system</scope>
			<systemPath>${basedir}/lib/mytests.jar</systemPath>
		</dependency>
		<dependency>
			<groupId>internal</groupId>
			<artifactId>headquarters-jdbc-proxy-1.1-cn-share</artifactId>
			<version>UNKOWN</version>
			<scope>system</scope>
			<systemPath>${basedir}/lib/my-oracle.jar</systemPath>
